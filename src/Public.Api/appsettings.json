  "DocsUrl": "https://docs.staging-basisregisters.vlaanderen/",
  "ErrorMessageUrl": "https://staging-basisregisters.vlaanderen/foutmeldingen/{0}",
  "BaseName": "Production",

  "RegistryAuthUser": "x",
  "RegistryAuthPass": "x",

  "Extract": {
    "Region": "eu-west-1",
    "Bucket": "",
    "DestinationPath": "",
    "BundleName": "extract",
    "ExpiresInSeconds": 60
